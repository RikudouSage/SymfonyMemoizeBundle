{
    "name": "rikudou/memoize-bundle",
    "description": "Memoize attribute for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rikudou\\MemoizeBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dominik Chrastecky",
            "email": "dominik@chrastecky.cz"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^5.4|^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpstan/phpstan": "^1.4"
    },
    "scripts": {
        "fixer": "php-cs-fixer fix --verbose --allow-risky=yes",
        "phpstan": "phpstan analyse --level=max src"
    }
}
